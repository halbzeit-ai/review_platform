{
  "name": "startup-review-platform",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "axios": "^1.6.5",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.6.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "recharts": "^3.1.0"
  },
  "proxy": "http://65.108.32.143:8000",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:fast": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test src/__tests__/unit --watchAll=false",
    "test:integration": "react-scripts test src/__tests__/integration --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --testResultsProcessor=jest-junit",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "msw": "^1.3.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|@mui|@emotion)/)"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.(test|spec).{js,jsx,ts,tsx}"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/__tests__/**",
      "!src/**/*.test.js"
    ]
  },
  "overrides": {
    "typescript": "^4.9.5"
  }
}
